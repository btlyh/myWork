	</method> -->
	<!--战斗开始-->
	<method name="setPort" ref="service">
		<field class="com.cambrian.dfhm.GlobalConst" name="COMMAND_BATTLE_START"/>
		<obj class="com.cambrian.dfhm.battle.command.MatchStartCommand"></obj>
	</method>
	<!--战斗结束相关服务
