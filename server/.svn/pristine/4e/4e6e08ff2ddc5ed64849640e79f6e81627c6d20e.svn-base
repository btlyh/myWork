package com.cambrian.dfhm.friend.command;

import com.cambrian.common.net.ByteBuffer;
import com.cambrian.dfhm.GlobalConst;
import com.cambrian.dfhm.PlayerCommand;
import com.cambrian.dfhm.common.Player;
import com.cambrian.dfhm.message.Message;
import com.cambrian.dfhm.message.MessageManager;
import com.cambrian.game.Session;

/**
 * ÀàËµÃ÷£º
 * @author£ºSebastian
 *
 */
public class RefuseApplyCommand extends PlayerCommand
{
	@Override
	public void handle(Session session,Player player,ByteBuffer data)
	{
		// TODO Auto-generated method stub
		long userid=data.readLong();
		data.clear();
		Message message=new Message();
		message.scr=player.getUserId();
		message.dest=userid;
		message.pid=GlobalConst.PROCESS_NULL;
		MessageManager.instance.send(message);
	}

}
