package com.cambrian.dfhm.battle.command;

import com.cambrian.common.net.ByteBuffer;
import com.cambrian.common.net.Command;
import com.cambrian.common.net.DataAccessException;
import com.cambrian.common.net.NioTcpConnect;
import com.cambrian.dfhm.Lang;
import com.cambrian.dfhm.common.Player;
import com.cambrian.game.Session;

/**
 * 类说明：
 * @author：Sebastian
 * 
 */
public class GetStageCommand extends Command {

	@Override
	public void handle(NioTcpConnect connect, ByteBuffer data) {
		Session session = connect.getSession();
		if(log.isDebugEnabled())
			log.debug("session = "+session);
		if(session == null) throw new DataAccessException(601, Lang.F9000_SDE);
		Player player = (Player)session.getSource();
		if(player == null) throw new DataAccessException(601, Lang.F9000_SDE);
		int type = data.readInt();//0=主线,1=挑战,2=穿越,3=活动
		data.clear();
	}
}
