<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objects>
	<!--
	NPC配置~！
	sid					唯一标签
	class				程序使用
	battleCardList		[怪物1，怪物2]NPC中的怪物卡牌
	index				NPC的位置
	needToken 			攻击此NPC所需要的军令
	type				NPC的类型[1主线,2挑战,3穿越,4活动]
	instancingSid 		当前NPC所属关卡SID
	openDay				此NPC是否开放(活动副本用)
	attConfine			此NPC的攻击次数限制
	isBoss				此NPC是否为BOSS 1是 0否
	nextIndex			下一个普通NPC的位置 如果为0表示最高副本
	hardIndex			开启挑战NPC的位置    如果为0则不配置
	crossIndex			开启穿越NPC的位置    如果为0则不配置
	// 挑战成功要求: 只有挑战NPC才需配置
	assignCardId		指定武将
	isAllLive			是否需要武将全体存活 1是 0否
	assignRound			指定回合数
	cardCount			指定武将个数
	assignDamage		一击所需造成的伤害
	name 				NPC名称（服务端不需要）
	description			NPC描述（服务端不需要）
	-->
	
	<sample sid="100001" class="com.cambrian.dfhm.instancing.NPC" battleCardList="30001,30002,30003,30004,30005" index="1" needToken="10" type="1" instancingSid="110001"  isBoss="0" nextIndex="2" />
	<sample sid="100002" class="com.cambrian.dfhm.instancing.NPC" battleCardList="30001,30002,30003,30004,30005" index="2" needToken="20" type="1" instancingSid="110001"  isBoss="1" nextIndex="3" />	
	<sample sid="100003" class="com.cambrian.dfhm.instancing.NPC" battleCardList="30001,30002,30003,30004,30005" index="3" needToken="10" type="1" instancingSid="110002"  isBoss="0" nextIndex="4" />
</objects>